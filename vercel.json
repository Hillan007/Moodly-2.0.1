{
  "version": 2,
  "builds": [
    {
      "src": "api/**/*.py",
      "use": "@vercel/python"
    },
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "dist"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/auth/(.*)",
      "dest": "/api/auth.py"
    },
    {
      "src": "/api/mood",
      "dest": "/api/mood.py"
    },
    {
      "src": "/api/insights/(.*)",
      "dest": "/api/insights.py"
    },
    {
      "src": "/api/music/(.*)",
      "dest": "/api/music.py"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}